{
  "extends": "standard",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-use-before-define": "off",
    "no-fallthrough": "off",
    "array-callback-return": "off"
  }
}